# vi: ft=sshconfig shiftwidth=4 tabstop=4 expandtab
#
# This is a sample ssh configuration for accessing PCDS/ECS hosts.
# 
# This could be called a remote or "home" configuration.
#
# You should use it on your own laptop and *not* place this in your PCDS
# home directory configuration.

Host github
    HostName github.com
    ForwardX11 no
    PreferredAuthentications=publickey

Host !github
    ForwardAgent yes
    Compression yes
    ForwardX11 yes
    ForwardX11Trusted yes
    XAuthLocation /opt/X11/bin/xauth
    PreferredAuthentications=publickey

Host pslogin
    HostName pslogin.slac.stanford.edu
    # ControlMaster can allow you to multiplex pslogin connections!
    # ControlMaster auto
    # ControlPath ~/.SSH-%r@%h:%p

Host pslogin-arch
    HostName pslogin.slac.stanford.edu
    # Archiver management interface:
    # use http://localhost:17665/mgmt/ui/index.html
    LocalForward 17665 pscaa02:17665
    # Archiver API for programmatic access:
    LocalForward 17668 pscaa02:17668
    # Power monitoring: http://localhost:8089/ion
    LocalForward 8089 powermon:80
    # recipe metering: http://localhost:8090/RecipeAnalytics/Operations/Status3
    LocalForward 8090 recipesrv:80

Host psbuild-rhel7
    HostName psbuild-rhel7-01
    ProxyCommand ssh -XT pslogin nc %h %p

Host psdev
    HostName psdev
    ProxyCommand ssh -XT pslogin nc %h %p

# plcprog-console access
Host psbuild-plc 
    HostName psbuild-rhel7
    # This following ProxyCommand line may not be required depending on your ssh version:
    ProxyCommand ssh -XT pslogin nc %h %p
    LocalForward 3389 plcprog-console:3389
 
# pscron - used for scheduling cron job tasks
Host pscron
    ProxyCommand ssh -XT pslogin nc %h %p

# Additional hosts from netconfig, hopping through psbuild-rhel7:
